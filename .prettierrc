{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "printWidth": 120,
  "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "^(@/types/(.*)$)|^(@/types$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@/utils/(.*)$)|^(@/utils$)",
    "^(.*)utils",
    "",
    "^(@/navigation/(.*)$)|^(@/navigation$)",
    "",
    "^(@/components/(.*)$)|^(@/components$)",
    "^(.*)components",
    "",
    "^(@/hooks/(.*)$)|^(@/hooks$)",
    "^(.*)hooks",
    "",
    "^(@/abi/(.*)$)|^(@/abi$)",
    "",
    "^(@/assets/(.*)$)|^(@/assets$)",
    "",
    "^(@/consts/(.*)$)|^(@/consts$)",
    "^(.*)consts",
    "",
    "^(@/data/(.*)$)|^(@/data$)",
    "^(.*)data",
    "",
    "^(@/schemas/(.*)$)|^(@/schemas$)",
    "",
    "^(@/styles/(.*)$)|^(@/styles$)",
    "",
    "^[./]"
  ]
}
